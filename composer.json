{
    "name": "sqills/payum-icepay",
    "type": "library",
    "description": "Icepay gateway for Payum",
    "keywords": ["payment", "payum", "icepay"],
    "homepage": "http://payum.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Pietrusinski",
            "email": "mark.pietrusinski@sqills.com"
        },
        {
            "name":     "Payum project",
            "homepage": "http://payum.org/"
        }
    ],
    "require": {
        "payum/core": "^1.3",
        "icepay/api": "dev-master",
        "doctrine/collections": "^1.3.0",
        "jms/serializer": "^1.3.0"
    },
    "require-dev": {
        "payum/core": "^1.3@dev",
        "php-http/guzzle6-adapter": "^1.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "Payum\\Icepay": "" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "target-dir": "Payum/Icepay",
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        }
    }
}
